<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<?import sample.AutocompletionlTextField?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #D2E3F5;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OldPatient">
   <children>
      <Label layoutX="53.0" layoutY="50.0" prefHeight="32.0" prefWidth="84.0" text="Name" >
      <font>
         <Font name="DejaVu Sans Bold" size="13.0" />
      </font></Label>
      <Label layoutX="53.0" layoutY="103.0" text="Mobile no." >
      <font>
         <Font name="DejaVu Sans Bold" size="13.0" />
      </font></Label>
      <AutocompletionlTextField fx:id="number" layoutX="182.0" layoutY="99.0" prefHeight="26.0" prefWidth="162.0" promptText="Enter the mobile number" />
      <Button fx:id="searchButton" layoutX="229.0" layoutY="230.0" mnemonicParsing="false" opacity="0.7" onAction="#searchButtonPushed" text="Search" />
      <Button fx:id="backButton" layoutX="33.0" layoutY="331.0" mnemonicParsing="false" opacity="0.7" onAction="#backButtonPushed" text="Back" />
      <AutocompletionlTextField fx:id="name" layoutX="182.0" layoutY="53.0" promptText="Enter the name here" />
      <Label fx:id="errorLabel" layoutX="187.0" layoutY="313.0" text="Please enter any one field" >
         <font>
            <Font name="DejaVu Sans Bold" size="13.0" />
         </font></Label>
   </children>
</AnchorPane>
